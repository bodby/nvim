{
  "environment": {
    "prefix": [ "environment", "begin" ],
    "body": [
      "\\begin{${1:name}}",
      "$0",
      "\\end{${1:name}}"
    ]
  },
  "section": {
    "prefix": "section",
    "body": [
      "\\section{${1:text}}",
      "$0"
    ]
  },
  "subsection": {
    "prefix": "subsection",
    "body": [
      "\\subsection{${1:text}}",
      "$0"
    ]
  },
  "subsubsection": {
    "prefix": "subsubsection",
    "body": [
      "\\subsubsection{${1:text}}",
      "$0"
    ]
  },
  "paragraph": {
    "prefix": "paragraph",
    "body": "\\paragraph{${1:text}} $0"
  },
  "subparagraph": {
    "prefix": "subparagraph",
    "body": "\\subparagraph{${1:text}} $0"
  },
  "equation": {
    "prefix": "equation",
    "body": [
      "\\begin{equation} \\tag{${1:2.12}}",
      "\t$0",
      "\\end{equation}"
    ]
  },
  "algorithmic": {
    "prefix": [ "algorithmic", "pseudocode" ],
    "body": [
      "\\begin{algorithmic}[1]",
      "\t$0",
      "\\end{algorithmic}"
    ]
  },
  "align": {
    "prefix": "align",
    "body": [
      "\\begin{align} \\tag{${1:2.12}}",
      "\t${2:y} &= ${3:2x} \\\\\\",
      "\t&= ${4:x + x}",
      "\\end{align}$0"
    ]
  },
  "parentheses": {
    "prefix": "parentheses",
    "body": [
      "\\left(",
      "\t$0",
      "\\right)"
    ]
  },
  "brackets": {
    "prefix": "brackets",
    "body": [
      "\\left[",
      "\t$0",
      "\\right]"
    ]
  },
  "matrix": {
    "prefix": "bmatrix",
    "body": [
      "\\begin{bmatrix}",
      "\t${1:a} & ${2:b} \\\\\\",
      "\t${3:c} & ${4:d}",
      "\\end{bmatrix}$0"
    ]
  },
  "smallmatrix": {
    "prefix": [ "smallmatrix", "bsmallmatrix", "inline-matrix" ],
    "body": [
      "\\left(",
      "\t\\begin{smallmatrix}",
      "\t\t${1:a} & ${2:b} \\\\\\",
      "\t\t${3:c} & ${4:d}",
      "\t\\end{smallmatrix}",
      "\\right)$0"
    ]
  },
  "figure": {
    "prefix": "figure",
    "body": [
      "\\begin{figure}",
      "\t\\caption{${1:A shape.}}",
      "\\end{figure}$0"
    ]
  },

  "inline-equation": {
    "prefix": [ "expression", "inline-equation", "math-inline" ],
    "body": "\\\\($0\\\\)"
  },
  "fraction": {
    "prefix": "fraction",
    "body": "\\frac{${1:n}}{${2:d}}$0"
  },
  "integral": {
    "prefix": "integral",
    "body": "\\int^{${1:+}}_{${2:-}}$0"
  },
  "overset": {
    "prefix": "overset",
    "body": "\\overset{${1:label}}{${2:expr}}$0"
  },
  "underset": {
    "prefix": "underset",
    "body": "\\underset{${1:label}}{${2:expr}}$0"
  },
  "greater-or-equal": {
    "prefix": [ "greater-than-or-equal", "geq" ],
    "body": "\\geq"
  },
  "less-or-equal": {
    "prefix": [ "less-than-or-equal", "leq" ],
    "body": "\\leq"
  },
  "not-equal": {
    "prefix": [ "not-equal" ],
    "body": "\\neq"
  },
  "not": {
    "prefix": [ "not", "negation" ],
    "body": "\\neg"
  },
  "gets": {
    "prefix": [ "gets", "assignment" ],
    "body": "\\gets"
  },
  "bold-math": {
    "prefix": [ "math-bold-math", "math-strong-math" ],
    "body": "\\mathbf{${1:text}}$0"
  },
  "italic-math": {
    "prefix": [ "math-italic-math", "math-emphasis-math" ],
    "body": "\\mathit{${1:text}}$0"
  },
  "italic-math": {
    "prefix": [ "math-italic-math", "math-emphasis-math" ],
    "body": "\\mathit{${1:text}}$0"
  },

  "bold-text": {
    "prefix": [ "text-bold-text", "text-strong-text" ],
    "body": "\\textbf{${1:text}}$0"
  },
  "italic-text": {
    "prefix": [ "text-italic-text", "text-emphasis-text" ],
    "body": "\\textit{${1:text}}$0"
  },
  "teletype": {
    "prefix": [ "teletype", "monospace" ],
    "body": "\\texttt{${1:text}}$0"
  },
  "verb": {
    "prefix": [ "verb", "pre" ],
    "body": "\\verb|${1:text}|$0"
  },
  "underbrace": {
    "prefix": "underbrace",
    "body": "\\underbrace{${1:expr}}_{${2:label}}$0"
  },
  "overbrace": {
    "prefix": "overbrace",
    "body": "\\overbrace{${1:expr}}^{${2:label}}$0"
  },
  "citation": {
    "prefix": [ "citation", "cite", "reference" ],
    "body": "\\cite{${1:key}}$0"
  },

  "algorithmic-state": {
    "prefix": [ "algorithmic-state", "algorithmic-definition" ],
    "body": "\\State \\\\(${1:var} \\gets ${2:10}\\\\)"
  },
  "algorithmic-if": {
    "prefix": [ "algorithmic-if", "algorithmic-conditional" ],
    "body": [
      "\\If{${1:cond}}",
      "\t$0",
      "\\EndIf"
    ]
  },
  "algorithmic-else": {
    "prefix": "algorithmic-else",
    "body": "\\Else$0"
  },
  "algorithmic-elif": {
    "prefix": [ "algorithmic-elif", "algorithmic-elseif" ],
    "body": "\\ElsIf{${1:cond}}$0"
  },
  "algorithmic-while": {
    "prefix": "algorithmic-while",
    "body": [
      "\\While{${1:cond}}",
      "\t$0",
      "\\EndWhile"
    ]
  },
  "algorithmic-for-loop": {
    "prefix": "algorithmic-for-loop",
    "body": [
      "\\For{${1:var} \\gets ${2:1}}{${3:10}}",
      "\t$0",
      "\\EndFor"
    ]
  },
  "algorithmic-repeat": {
    "prefix": "algorithmic-repeat",
    "body": [
      "\\Repeat",
      "\t$0",
      "\\Until{${1:cond}}"
    ]
  },
  "algorithmic-forall": {
    "prefix": [ "algorithmic-forall", "algorithmic-map" ],
    "body": [
      "\\ForAll{${1:elem} \\in ${2:set}}",
      "\t$0",
      "\\EndFor"
    ]
  },
  "algorithmic-procedure": {
    "prefix": "algorithmic-procedure",
    "body": [
      "\\Procedure{${1:Name}}{${2:args}}",
      "\t$0",
      "\\EndProcedure"
    ]
  },
  "algorithmic-function": {
    "prefix": "algorithmic-function",
    "body": [
      "\\Function{${1:Name}}{${2:args}}",
      "\t$0",
      "\\EndFunction"
    ]
  },
  "algorithmic-comment": {
    "prefix": "algorithmic-comment",
    "body": "\\Comment{${1:Comment}}$0"
  },
  "algorithmic-line-comment": {
    "prefix": "algorithmic-line-comment",
    "body": "\\LComment{${1:Comment}}$0"
  },
  "algorithmic-wrap": {
    "prefix": [ "algorithmic-wrap", "algorithmic-newline", "algorithmic-blankline" ],
    "body": "\\Statex$0"
  }
}
