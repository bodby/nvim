{
  "meta": {
    "prefix": "meta",
    "body": [
      "\\title{${1:title}}",
      "\\date{${2:date}}",
      "\\author{${3:Baraa Homsi}}"
    ]
  },
  "environment": {
    "prefix": [ "environment", "begin" ],
    "body": [
      "\\begin{${1:name}}",
      "$0",
      "\\end{${1:name}}"
    ]
  },
  "section": {
    "prefix": "section",
    "body": [
      "\\section{${1:text}}",
      "",
      "$0"
    ]
  },
  "subsection": {
    "prefix": "subsection",
    "body": [
      "\\subsection{${1:text}}",
      "",
      "$0"
    ]
  },
  "subsubsection": {
    "prefix": "subsubsection",
    "body": [
      "\\subsubsection{${1:text}}",
      "",
      "$0"
    ]
  },
  "paragraph": {
    "prefix": "paragraph",
    "body": [
      "\\paragraph{${1:text}}",
      "",
      "\t$0"
    ]
  },
  "subparagraph": {
    "prefix": "subparagraph",
    "body": [
      "\\subparagraph{${1:text}}",
      "",
      "\t$0"
    ]
  },
  "equation": {
    "prefix": "equation",
    "body": [
      "\\begin{equation} \\tag{${1:2.12}}",
      "\t$0",
      "\\end{equation}"
    ]
  },
  "align": {
    "prefix": "align",
    "body": [
      "\\begin{align} \\tag{${1:2.12}}",
      "\t${2:y} &= ${3:2x}\\\\\\",
      "\t&= ${4:x + x}",
      "\\end{align}$0"
    ]
  },
  "parentheses": {
    "prefix": "parentheses",
    "body": [
      "\\left(",
      "\t$0",
      "\\right)"
    ]
  },
  "brackets": {
    "prefix": "brackets",
    "body": [
      "\\left[",
      "\t$0",
      "\\right]"
    ]
  },
  "matrix": {
    "prefix": "bmatrix",
    "body": [
      "\\begin{bmatrix}",
      "\t${1:a} & ${2:b}\\\\\\",
      "\t${3:c} & ${4:d}",
      "\\end{bmatrix}",
      "$0"
    ]
  },
  "smallmatrix": {
    "prefix": [ "smallmatrix", "bsmallmatrix", "inline-matrix" ],
    "body": [
      "\\left(",
      "\t\\begin{smallmatrix}",
      "\t\t${1:a} & ${2:b}\\\\\\",
      "\t\t${3:c} & ${4:d}",
      "\t\\end{smallmatrix}",
      "\\right)",
      "$0"
    ]
  },
  "figure": {
    "prefix": "figure",
    "body": [
      "\\begin{figure}",
      "\t\\caption{${1:A shape.}}",
      "\\end{figure}",
      "$0"
    ]
  },

  "fraction": {
    "prefix": "fraction",
    "body": "\\frac{${1:n}}{${2:d}}$0"
  },
  "integral": {
    "prefix": "integral",
    "body": "\\int^{${1:+}}_{${2:-}}$0"
  },
  "overset": {
    "prefix": "overset",
    "body": "\\overset{${1:label}}{${2:expr}}$0"
  },
  "underset": {
    "prefix": "underset",
    "body": "\\underset{${1:label}}{${2:expr}}$0"
  },

  "bold": {
    "prefix": [ "bold", "strong" ],
    "body": "\\textbf{${1:text}}$0"
  },
  "italic": {
    "prefix": [ "italic", "emphasis" ],
    "body": "\\textit{${1:text}}$0"
  },
  "teletype": {
    "prefix": [ "teletype", "monospace" ],
    "body": "\\texttt{${1:text}}$0"
  },
  "verb": {
    "prefix": [ "verb", "pre" ],
    "body": "\\verb|${1:text}|$0"
  },
  "underbrace": {
    "prefix": "underbrace",
    "body": "\\underbrace{${1:expr}}_{${2:label}}$0"
  },
  "overbrace": {
    "prefix": "overbrace",
    "body": "\\overbrace{${1:expr}}^{${2:label}}$0"
  }
}
